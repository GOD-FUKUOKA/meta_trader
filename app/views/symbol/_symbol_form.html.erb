<!-- Modal -->
<div id="modal-1" class="modal hide in modal-form modal-symbol tab-forms" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="false" style="display: block;">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
      <h3 id="myModalLabel" class="normal">Add (ili Edit) New Access</h3>
    </div>
    <div class="modal-body ">


<%= form_tag symbol_submit_path, :method => "POST", :remote => true do %>
  <%= hidden_field_tag "new_symbol", @sym.empty? %>
  <%= hidden_field_tag "symbol_key", @sym["symbol"] %>
  <%= tabbed_panel do |tp| %>
    
    <% tp.add_tab("Symbol") do %>
      <%= render "symbol/symbol_tab" %>
    <% end %>
    <% tp.add_tab("Filtration") do %>
      <%= render "symbol/filtration_tab" %>
    <% end %>
    <% tp.add_tab("Calculation") do %>
      <%= render "symbol/calculation_tab" %>
    <% end %>
    <% tp.add_tab("Swaps") do %>
      <%= render "symbol/swaps_tab" %>
    <% end %>
    <% tp.add_tab("Sessions") do %>
      <%= render "symbol/sessions_tab" %>
    <% end %>

  <% end %>

    <div class="form-actions">
      <%= submit_tag"Submit", :class => "btn btn-primary" %>
    </div> 

<% end %>

    </div><!--/modal-body-->
    
  </div><!--/modal-1-->