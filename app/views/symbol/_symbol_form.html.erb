<%= form_tag symbol_submit_path, :method => "POST", :remote => true do %>
  <%= hidden_field_tag "new_symbol", @sym.empty? %>
  <%= hidden_field_tag "symbol_key", @sym["symbol"] %>
  <%= tabbed_panel :class => "blah" do |tp| %>
    <% tp.add_tab("Symbol") do %>
      <%= render "symbol/symbol_tab" %>
    <% end %>
    <% tp.add_tab("Filtration") do %>
      <%= render "symbol/filtration_tab" %>
    <% end %>
    <% tp.add_tab("Calculation") do %>
      <%= render "symbol/calculation_tab" %>
    <% end %>
    <% tp.add_tab("Swaps") do %>
      <%= render "symbol/swaps_tab" %>
    <% end %>
    <% tp.add_tab("Sessions") do %>
      <%= render "symbol/sessions_tab" %>
    <% end %>
    <%= submit_tag %>
  <% end %>
<% end %>