$("#data_server_new_record").val("true");
<% if @result %>
  $(".data_server_table .data_server_row[data-position='<%= @position%>']").first().remove();
  $(".data_server_table .data_server_row").each(function(ix,el){
    if( parseInt($(el).attr("data-position")) > <%= @position %> ){
      $(el).attr("data-position", parseInt($(el).attr("data-position")) - 1);
      $(el).find("td a").first().attr("href", "/tabs/data_server/"+$(el).attr("data-position").toString());
    }
  });
<% end %>